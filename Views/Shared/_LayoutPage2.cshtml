<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    @*<script src="~/Scripts/jquery-3.6.4.min.js"></script>*@
    @*<script src="~/Scripts/jquery-3.0.0.min.js"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    <title>Dashboard</title>
</head>
<body>

    <div class="row">


        <div class="col-lg-2-md-2 col-sm-2 col-xs-2" style="height:1000px;border:1px solid gray;background-color:cadetblue">

            <div class="list-group">

                <div class="list-group-item">
                    @Html.ActionLink("Add Category", "Add_Category", new { @class = "list-group-item" })
                </div>
                <div class="list-group-item">
                    @Html.ActionLink("Add Questions", "AddQuestions", new { @class = "list-group-item" })
                </div>
                <div class="list-group-item">
                    @Html.ActionLink("Report", "Report", new { })
                </div>

            </div>

            <div class="list-group-item">
                @Html.ActionLink("Log Out", "LogOut", new { @class = "list-group-item" })
            </div>

        </div>

        <div class="col-lg-10-md-10 col-sm-10 col-xs-10" style="height:1000px;border:1px solid gray">

            @RenderBody()


        </div>


    </div>


</body>
</html>
